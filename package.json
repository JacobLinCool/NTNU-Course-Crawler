{
    "name": "ntnu-course-crawler",
    "version": "0.1.0",
    "description": "Crawler for Courses on National Taiwan Normal University's Website",
    "main": "dist/index.js",
    "scripts": {
        "prepare": "npm run -s build",
        "start": "node dist",
        "dev": "tsup --watch",
        "build": "tsup",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' --ignore-path .gitignore",
        "lint": "eslint ."
    },
    "keywords": [
        "NTNU",
        "National Taiwan Normal University",
        "Crawler"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "types": "lib/index.d.ts",
    "files": [
        "lib"
    ],
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/node": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "tsup": "^5.11.11",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@jacoblincool/puddle": "^0.1.1",
        "commander": "^9.2.0",
        "ntnu-course": "^0.1.3"
    }
}
