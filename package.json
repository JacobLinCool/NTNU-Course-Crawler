{
    "name": "ntnu-course-crawler",
    "version": "0.1.0",
    "description": "Crawler for Courses on National Taiwan Normal University's Website",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "prepare": "npm run -s build",
        "start": "node dist",
        "dev": "tsup --watch",
        "build": "tsup",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' --ignore-path .gitignore",
        "lint": "eslint ."
    },
    "keywords": [
        "NTNU",
        "National Taiwan Normal University",
        "Crawler"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "types": "lib/index.d.ts",
    "files": [
        "lib"
    ],
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.0.0",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "prettier": "^2.7.1",
        "tsup": "^6.3.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@jacoblincool/puddle": "^0.1.1",
        "chalk": "^5.1.2",
        "commander": "^9.4.1",
        "course-pack": "1.0.0-alpha.4",
        "cuid": "^2.1.8",
        "log-update": "^5.0.1",
        "ntnu-course": "^0.1.5"
    }
}
